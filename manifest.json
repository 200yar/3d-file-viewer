{
	"name": "3D File Viewer",
	"version": "1.2.0",
	"manifest_version": 2,
	"offline_enabled": true,
	"app": {
		"background": {
			"scripts": [
				"background.js"
			],
			"persistent": false
		}
	},
	"file_handlers": {
		"text": {
			"extensions": [
				"glb",
				"obj",
				"stl",
				"ply"
			]
		}
	},
	"icons": {
		"128": "icon.png" 
	}
}